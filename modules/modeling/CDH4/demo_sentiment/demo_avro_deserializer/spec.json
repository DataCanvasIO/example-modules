{
    "Name" : "avro_deserializer",
    "Description" : "A module to deserialize avro data",
    "Version" : "0.1",
    "CategoryTags": ["project_sentiment", "CDH4"],
    "Cmd" : "python main.py",
    "Param" : {
        "hdfs_root": {
            "Default": "$hdfs_root",
            "Type": "string"
        },
        "yarn_resourcemanager": {
            "Default": "127.0.0.1:8032",
            "Type": "string"
        },
        "yarn_resourcemanager_scheduler": {
            "Default": "127.0.0.1:8030",
            "Type": "string"
        },
        "AWS_ACCESS_KEY_ID": {
            "Default": "",
            "Type": "string"
        },
        "AWS_ACCESS_KEY_SECRET": {
            "Default": "",
            "Type": "string"
        },
        "S3_BUCKET": {
            "Default": "",
            "Type": "string"
        }
    },
    "Input" : {
        "avro_path": [
            "hdfs.avro"
        ]
    },
    "Output" : {
        "output_path": [
            "s3.dir"
        ]
    }
}
