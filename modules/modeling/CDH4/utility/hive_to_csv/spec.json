{
    "Name": "Hive_to_CSV",
    "Description": "A hive to csv downloader",
    "Version": "0.2",
    "CategoryTags": ["CDH4", "utility"],
    "Cmd": "python main.py",
    "Param": {
        "HiveServer2_Host": {
            "Default": "",
            "Type": "string"
        },
        "HiveServer2_Port": {
            "Default": "",
            "Type": "string"
        },
        "Where_Clause": {
            "Default": "",
            "Type": "string"
        },
        "LIMIT" : {
            "Default": 0,
            "Type" : "integer"
        },
        "SCHEMA" : {
            "Default" : "*",
            "Type" : "string"
        }
    },
    "Input": {
        "input_tbl": [
            "hive.table"
        ]
    },
    "Output": {
        "O": [
            "csv"
        ]
    }
}
