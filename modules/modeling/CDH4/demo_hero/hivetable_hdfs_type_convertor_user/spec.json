{
    "Name": "demo_hivetable_hdfs_type_convertor_user",
    "Description": "Convert HDFS to a hive.table.",
    "Version": "0.1",
    "CategoryTags": [
        "demo_hero",
        "CDH4"
    ],
    "Cmd": "/usr/bin/python main.py",
    "Param": {
        "hdfs_root": {
            "Default": "$hdfs_root",
            "Type": "string"
        },
        "HiveServer2_Host": {
            "Default": "$HiveServer2_Host",
            "Type": "string"
        },
        "HiveServer2_Port": {
            "Default": "$HiveServer2_Port",
            "Type": "string"
        },
        "FILE_DIR": {
            "Default": "./resources/files",
            "Type": "string"
        },
        "UDF_DIR": {
            "Default": "./resources/udfs",
            "Type": "string"
        },
        "SCHEMA": {
            "Default": "",
            "Type": "string"
        }
    },
    "Input": {
        "input": [
            "hdfs.user"
        ]
    },
    "Output": {
        "output": [
            "hive.table"
        ]
    }
}