{
    "Name" : "datasource_to_s3",
    "Description" : "A datasource to s3 uploader",
    "Version" : "0.1",
    "CategoryTags": [ "ETL" ],
    "Cmd" : "python main.py",
    "Param": {
        "AWS_ACCESS_KEY_ID": {
            "Default": "$AWS_ACCESS_KEY_ID",
            "Type": "string"
        },
        "AWS_ACCESS_KEY_SECRET": {
            "Default": "$AWS_ACCESS_KEY_SECRET",
            "Type": "string"
        },
        "S3_BUCKET": {
            "Default": "$S3_BUCKET",
            "Type": "string"
        }
    },
    "Input": {
        "DS": [ "datasource" ]
    },
    "Output": {
        "dest_s3": [
            "datasource.s3"
        ]
    }

}
